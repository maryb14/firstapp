<div class="row">
  <div class="col-md-6 col-md-offset-3 grammar-category-view">
    <h2><%= @vocabulary_category.name %></h2>
    <div class="grammar-content">
      <%= @vocabulary_category.content.html_safe %>
    <div>
    <table>
      <tbody>
        <% @category_items.each do |c| %>
          <tr>
            <td>
            	<%= image_tag c.image_url(:thumb), class: "modal_image", data: {id: c.id} %>
            </td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>


<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
    
</div>